{
	"name": "admin-ui",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"bundle-report": "webpack-bundle-analyzer integration/stats-es2015.json",
		"start": "ng serve",
		"proxy": "ng serve --live-reload false --proxy-config proxy.conf.json --port 4200",
		"build.ui": "ng build",
		"build.server-development": "cd ./server && rimraf ./dist && tsc",
		"build.server-inegration": "cd ./server && tsc && cp -rf ./dist/* ./../integration/ && rimraf ./dist",
		"build.server-production": "cd ./server && tsc && cp -rf ./dist/* ./../production/ && rimraf ./dist",
		"integration": "npm run lint && npm run test && rimraf ./integration && ng build --stats-json --prod --build-optimizer -c integration && mv ./dist/ ./integration/ && npm run build.server-inegration && cross-env NODE_ENV=integration node ./scripts/gzip_files && cross-env NODE_ENV=integration node ./scripts/version.js",
		"production": "rimraf ./production && ng build --prod --aot --build-optimizer -c production && mv ./dist/ ./production/ && npm run build.server-production && cross-env NODE_ENV=production node ./scripts/gzip_files && cross-env NODE_ENV=production node ./scripts/version.js",
		"galaxy-ui-dev": "cross-env NODE_ENV=development nodemon -e ./server/**/*.ts -x ts-node ./server/server.ts --dev",
		"galaxy-ui-integration": "cross-env NODE_ENV=integration node ./integration/server.js --prod",
		"galaxy-ui-production": "cross-env NODE_ENV=production node ./production/server.js --prod",
		"test": "npm run test.server && npm run test.ui",
		"test.ui": "echo '---Unit Tests UI start---' && ng test && echo '---Unit Tests UI end---'",
		"test.server": "cd ./server && cross-env NODE_ENV=development ./../node_modules/.bin/mocha --no-watch --no-progress --browsers=ChromeHeadlessCI",
		"lint": "ng lint",
		"e2e": "ng e2e"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "9.0.7",
		"@angular/cdk": "9.1.3",
		"@angular/common": "9.0.7",
		"@angular/compiler": "9.0.7",
		"@angular/core": "9.0.7",
		"@angular/forms": "9.0.7",
		"@angular/platform-browser": "9.0.7",
		"@angular/platform-browser-dynamic": "9.0.7",
